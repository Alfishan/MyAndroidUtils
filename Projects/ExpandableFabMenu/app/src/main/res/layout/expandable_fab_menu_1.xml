<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:background="@color/transparent_75_dark"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             tools:layout_editor_absoluteY="25dp"
                                             tools:layout_editor_absoluteX="0dp">

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            style="@style/CloseFab"
            android:id="@+id/fab_menu_close"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintVertical_bias="0.504"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_build_white_36dp"
            android:id="@+id/fab_menu_service"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenuService"
            android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toTopOf="@+id/fab_menu_close"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="parent"
            />

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:rippleColor="@color/black_magic"
            app:srcCompat="@drawable/ic_local_gas_station_white_36dp"
            android:id="@+id/fab_menu_refueling"
            app:backgroundTint="@color/fabMenuRefueling"
            android:layout_marginEnd="24dp"
            app:layout_constraintLeft_toRightOf="@+id/fab_menu_close"
            android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_credit_card_white_36dp"
            android:id="@+id/fab_menu_expense"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenuExpense"
            android:layout_marginEnd="32dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/fab_menu_close"
            android:layout_marginRight="32dp"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_extension_white_36dp"
            android:id="@+id/fab_menu_expense_4"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenu4"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/fab_menu_close"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_explore_white_36dp"
            android:id="@+id/fab_menu_expense_5"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenu5"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/fab_menu_expense"
            app:layout_constraintRight_toLeftOf="@+id/fab_menu_expense_4"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_settings_applications_white_36dp"
            android:id="@+id/fab_menu_expense_6"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenu6"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/fab_menu_expense"
            app:layout_constraintRight_toLeftOf="@+id/fab_menu_service"
            android:layout_marginRight="16dp"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/ic_lightbulb_outline_white_36dp"
            android:id="@+id/fab_menu_expense_7"
            app:rippleColor="@color/black_magic"
            app:backgroundTint="@color/fabMenu7"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/fab_menu_refueling"
            app:layout_constraintLeft_toRightOf="@+id/fab_menu_service"
            android:layout_marginLeft="16dp"/>

    <android.support.design.widget.FloatingActionButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:fabSize="mini"
            app:rippleColor="@color/black_magic"
            app:srcCompat="@drawable/ic_favorite_border_white_36dp"
            android:id="@+id/fab_menu_expense_8"
            app:backgroundTint="@color/fabMenu8"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/fab_menu_refueling"
            app:layout_constraintLeft_toRightOf="@+id/fab_menu_expense_4"
            android:layout_marginLeft="16dp"/>

    <!--  <android.support.constraint.Group

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="fab_menu_expense,fab_menu_expense_label"
            android:id="@+id/group_expense"/>

    <android.support.constraint.Group
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="fab_menu_refueling,fab_menu_refueling_label"
            android:id="@+id/group_refueling"/>

    <android.support.constraint.Group
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:constraint_referenced_ids="fab_menu_service,fab_menu_service_label"
            android:id="@+id/group_service"/>-->

</android.support.constraint.ConstraintLayout>